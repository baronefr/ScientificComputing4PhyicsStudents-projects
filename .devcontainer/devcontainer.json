{
    "name": "AlmaLinux 9 Development Container",
    "build": {
        "dockerfile": "Dockerfile",
    },
    "runArgs": ["--name", "almalinux9-container"],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
        "ms-vscode.cpptools",
        "julialang.language-julia"
    ],
    "remoteUser": "root"
}